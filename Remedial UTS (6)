#include <iostream>
using namespace std;
int main()
{
    string bil[4][4];
    
    for (int a = 0; a<4; b++)
    {
        for (int b = 0; b<4; b++)
        {
            cout<<"Matriks "<<a<<","<<b<< " = ";
            cin>>bil[a][b];
        }
    }
    
    cout<<"\n =================================================== "<<endl;
    cout<<"Hasil Yang Anda Masukkan, Tampilannya Adalah "<<endl;
    cout<<"\n =================================================== "<<endl;
    
    for (int a = 0; a<4; a++)
    {
        for (int b = 0; b<4; b++)
        {
            cout<<bil[a][b]<<" | ";
        }
        cout<<endl;
    }
}
